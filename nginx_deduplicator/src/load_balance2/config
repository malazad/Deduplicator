ngx_addon_name=ngx_http_upstream_hash_module3

if test -n "$ngx_module_link"; then
  ngx_module_type=HTTP
  ngx_module_name=ngx_http_upstream_hash_module3
  ngx_module_srcs="$ngx_addon_dir/ngx_http_upstream_hash_module3.c"
  . auto/module
else
	HTTP_MODULES="$HTTP_MODULES ngx_http_upstream_hash_module3"
	NGX_ADDON_SRCS="$NGX_ADDON_SRCS $ngx_addon_dir/ngx_http_upstream_hash_module3.c"
fi
